{
  "name": "enabler",
  "private": true,
  "version": "4.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "serve public -l 8080 --cors",
    "test": "jest --detectOpenHandles --clearMocks --colors --config jest.config.cjs --ci --ci --reporters=default --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "npm run build && serve public -l 3000 --cors",
    "lint": "eslint . --ext .ts",
    "prettier:check": "prettier --check '**/*.{js,ts}'",
    "fix": "eslint . --ext .ts --fix && prettier --write '**/*.{js,ts}'"
  },
  "dependencies": {
    "@jest/environment": "^29.7.0",
    "node-fetch": "^3.3.2",
    "serve": "14.2.3"
  },
  "devDependencies": {
    "@types/node": "22.5.5",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass": "1.79.1",
    "ts-jest": "^29.2.5",
    "typescript": "5.6.2",
    "vite": "5.4.6",
    "vite-plugin-css-injected-by-js": "3.5.1",
    "prettier": "^3.3.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-unused-imports": "3.2.0"
  },
  "overrides": {
    "path-to-regexp": "3.3.0"
  }
}
